
"""
Environment configuration example
Copy to .env and update values
"""

# Environment
ENVIRONMENT=development
DEBUG=false
LOG_LEVEL=INFO

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# DuckDB Configuration
DUCKDB_PATH=crypto_data.duckdb
DUCKDB_READ_ONLY=false

# Binance API (if you have API keys - optional)
BINANCE_API_KEY=
BINANCE_API_SECRET=

# Collector Configuration
COLLECTOR_SYMBOLS=BTCUSDT,ETHUSDT,BNBUSDT,SOLUSDT,XRPUSDT
COLLECTOR_INTERVALS=1m,5m,15m,1h,4h,1d,1w

# Feature Flags
USE_NEW_INFRASTRUCTURE=true
ENABLE_WEBSOCKET=true
ENABLE_REST_COLLECTOR=true
ENABLE_DATA_PROCESSOR=true

# Celery Configuration
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# Streamlit Configuration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_HEADLESS=true